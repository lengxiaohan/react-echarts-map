html,body {
	margin: 0;
	padding: 0;
	font-family: "Segoe UI", "Lucida Grande", Helvetica, Arial,
		"Microsoft YaHei ui", FreeSans, Arimo, "Droid Sans",
		"wenquanyi micro hei", "Hiragino Sans GB", "Hiragino Sans GB W3",
		Arial, sans-serif;
	width: 100%;
	height: 100%;
	overflow: hidden;
	-webkit-box-sizing:border-box;
	box-sizing:border-box;
}

.container-box{
	width: 100%;
	height: 100%;
	display: -webkit-box;
    display: -moz-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-flex-direction: column;
    -moz-flex-direction: column;
    -ms-flex-direction: column;
    -o-flex-direction: column;
    flex-direction: column;
}

section {
	margin: 0 auto;
	width: 92%;
	display: -webkit-box;
    display: -moz-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-flex-direction: column;
    -moz-flex-direction: column;
    -ms-flex-direction: column;
    -o-flex-direction: column;
    flex-direction: column;
	-webkit-box-flex: 1;
	-moz-box-flex: 1;
	-webkit-flex: 1;
	-ms-flex: 1;
	flex: 1;
}
.header{
	margin: 0 auto;
	width: 98%;
	background:#ea622f;
	height:50px;
	line-height:50px;
	text-align:center;
	color:#fff;
	font-size:1.6rem;
	font-weight:bold;
	margin-top:5px;
	letter-spacing:8px;
}

.sec-title {
	margin: 10px auto;
	font-size: 0.9em;
	position: relative;
	top: 0;
	color: #aeb6c1;
	text-align: center;
	width: 100%;
	background: url("../img/shis.png") no-repeat;
	background-size: 100% 100%;
	height: 40px;
}

.sec-title span{
	position: relative;
	top: 4px;
	font-weight: bold;
}

.sec-title .title {
	text-align: center;
	color: #fff;
	margin-left: 9px;
	font-size: 1.2em;
}

.sec-title .yuan {
	text-align: center;
	font-size: 1.4em;
	color: #fff;
}

.sec-title .t_num {
	color: #ffc000;
	font-size: 1.3em;
}

.sec-map {
	width: 100%;
	-webkit-box-flex: 1;
	-moz-box-flex: 1;
	-webkit-flex: 1;
	-ms-flex: 1;
	flex: 1;
}

.legend {
	list-style: none;
	margin: 0;
	padding: 0;
	position: fixed;
	bottom: 4%;
	left: 4%;
	display:none;
}
.legendShow{
	display: block;
}
.legend li {
	color: #fff;
	font-size: 0.8em;
	line-height: 0.1
}

.legend li img {
	position: relative;
	top: 5px;
}