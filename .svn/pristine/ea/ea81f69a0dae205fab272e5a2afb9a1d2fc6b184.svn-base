// 引入z.less库
@import "plus/z";


// 初始化页面
.reset();

html,
body{
	.maxBody();
	overflow: hidden;
}
.container{
	.maxBody();
	canvas{
		.maxBody(100%,91.6%);
		opacity: 0;
	}
	header{
		width: 100%;
	    height: 8.4%;
	    .d-flex-h();
		.flex-hc();
		.flex-vc();
	    background:url('../img/header-bg.png');
	    background-size: auto 100%;
	    color: #fff;
	    font-size: 5vh;
	    .font-fm();
	}
}