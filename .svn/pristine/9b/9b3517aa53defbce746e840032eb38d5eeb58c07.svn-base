html,
body{
	margin: 0;
	padding: 0;
	width: 100%;
	height:100%;
}
.container{
	height:98%;
    width: 98%;
	overflow: hidden;
	background:rgba(0,9,56,.8);
    opacity: 0;
}
.head_title{
    margin: 1%; 
	height:12%;
	background:url('../img/titleBg.png');
	background-size:auto 100%;
	font-size:7vh;
	font-family:'微软雅黑';
	color:#fff;
	display: -webkit-box;
    display: -moz-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
    -moz-align-items: center;
    -ms-align-items: center;
    -o-align-items: center;
    align-items: center;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
    -moz-justify-content: center;
    -ms-justify-content: center;
    -o-justify-content: center;
    justify-content: center;
}
.myCanvas{
    width: 100%;
	height:86%;
}
.head-num{
	color:#ff3333;
}