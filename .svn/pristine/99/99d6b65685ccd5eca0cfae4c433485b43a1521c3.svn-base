html,body {
	margin: 0;
	padding: 0;
	font-family: "Segoe UI", "Lucida Grande", Helvetica, Arial,
		"Microsoft YaHei ui", FreeSans, Arimo, "Droid Sans",
		"wenquanyi micro hei", "Hiragino Sans GB", "Hiragino Sans GB W3",
		Arial, sans-serif;
	width: 100%;
	height: 100%;
	overflow: hidden;
	-webkit-box-sizing:border-box;
	box-sizing:border-box;
}

.timer {
	text-align: right;
	position: absolute;
	right: 60px;
	top: -13px;
	color: #fff;
	font-size: 19px;
}

.containermy {
	margin: 0 auto;
	width: 92%;
}
.container-header{
	margin: 0 auto;
	width: 98%;
	background:#ea622f;
	height:50px;
	line-height:50px;
	text-align:center;
	color:#fff;
	font-size:1.6rem;
	font-weight:bold;
	margin-top:5px;
	letter-spacing:8px;
}
.containermy .jiankongA {
	width: 17%;
	float: left;
	position: relative;
	top: 120px;
	right: 40px;
}

.jiaoyitext {
	margin: 10px auto;
	font-size: 0.9em;
	position: relative;
	top: 0;
	color: #aeb6c1;
	text-align: center;
	width: 100%;
	background: url("../img/shis.png") no-repeat;
	background-size: 100% 100%;
	height: 40px;
}

.jiankongA .titlejiao {
	color: #fff;
	font-size: 1.8em;
	width: 300px;
}

.jiaoyitext .yuansty {
	text-align: center;
	font-size: 1.4em;
	color: #fff;
	position: relative;
	top: 4px;
	left: 0px;
	font-weight: bold;
}

.jiaoyitext .t_num {
	color: #ffc000;
	position: relative;
	top: 4px;
	font-size: 1.3em;
	font-weight: bold;
}

.jiankongB {
	width: 100%;
	height: 100%
}

.jiankongB .mapsty {
	position: relative;
	width: 100%;
	height: 100%
}

.jiankongB .mapsty2 {
	height: 350px;
	width: 100%;
	position: relative;
	top: 10px;
}

.wllse {
	text-align: center;
	color: #fff;
	margin-left: 9px;
	position: relative;
	top: 4px;
	font-weight: bolder;
	font-size: 1.2em;
}

.tubiao {
	list-style: none;
	margin: 0;
	padding: 0;
	position: fixed;
	bottom: 4%;
	left: 4%;
	display:none;
}

.tubiao li {
	color: #fff;
	font-size: 0.8em;
	line-height: 0.1
}

.tubiao li img {
	position: relative;
	top: 5px;
}