html,body {
	margin: 0;
	padding: 0;
	font-family: "Segoe UI", "Lucida Grande", Helvetica, Arial,
		"Microsoft YaHei ui", FreeSans, Arimo, "Droid Sans",
		"wenquanyi micro hei", "Hiragino Sans GB", "Hiragino Sans GB W3",
		Arial, sans-serif;
	width: 100%;
    height: 100%;
	border-left: 2px rgba(0, 153, 255, 0.5) solid;
	border-right: 2px rgba(0, 153, 255, 0.5) solid;
	border-bottom: 2px rgba(0, 153, 255, 0.5) solid;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}
.containerBox{
	height: 100%;
}
.container{
	background: rgba(0, 0, 51, 0.7);
	display: -webkit-box;
	display: -moz-box;
	display: -ms-flexbox;
	display: -webkit-flex;
	display: flex;
	-webkit-flex-wrap: nowrap;
	-ms-flex-wrap: nowrap;
	flex-wrap: nowrap;
	height: 100%;
	flex-direction: column;
}

.header {
	height: 6%;
	width: 100%;
	position: relative;
}

.head-left {
	width: 25%;
	height: 100%;
	border-top: 1px rgba(0, 153, 255, 0.5) solid;
	float: left;
	position: relative;
	margin-top: 1px;
}

.head-con {
	width: 50%;
	height: 100%;
	float: left;
	box-sizing: border-box;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
	position: relative;
}

.head-con p {
	position: absolute;
	top: 0;
	width: 100%;
	height: 100%;
	margin: 0 auto;
	text-align: center;
	z-index: 4;
	color: #fff;
	font-size: 3.5vh;
}

.head-right {
	width: 25%;
	height: 100%;
	border-top: 1px rgba(0, 153, 255, 0.5) solid;
	float: left;
	position: relative;
	margin-top: 1px;
}

.head-con img {
	width: 100%;
	height: 100%;
	position: relative;
	z-index: 2;
}

.navBox {
	float: left;
	width: 33%;
	height: 100%;
	-webkit-flex: 1;
	display: -webkit-flex;
	display: flex;
	box-sizing: border-box;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
}

.navBox img {
	margin-left: 2em;
	width: 70%;
	height: 100px;
}

.navContainer {
	width: 100%;
	height: 14%;
	margin: 4px 0;
	position: relative;
	display: -webkit-flex;
	display: flex;
}

.navBoxList {
	width: 60%;
	height: 90%;
	position: relative;
	background: url(../img/top-bg.png);
	background-size: 100% 100%;
	box-sizing: border-box;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
	margin: auto;
}

.navBoxList p {
	color: #fff;
	margin:3px 0 0 3%;
	font-size: 3.5vh;
}

.navBoxList b{
	font-size: 3.5vh;
}

.navBoxList span {
	font-size: 2vh;
	color: #0089E8;
}

.boxListRight {
	position: relative;
	text-align: right;
	padding-right: 3%;
}

.navBoxList .success {
	color: #11F675
}

.navBoxList .delay {
	color: #E5B732
}

.navBoxList .errors {
	color: #FF3334
}

.sectionList {
	width: 100%;
	height: 74%;
	position: relative;
	display: -webkit-flex;
	display: flex;
}

.sectionList .sectionListBox {
	position: relative;
	width: 94%;
	height: 100%;
	margin: 0 auto;
	display: -webkit-flex;
	display: flex;
	flex-direction: column;
}

.sectionList img {
	position: relative;
	top: 6px;
	z-index: -5;
	width: 100%;
	height: 40px;
}

.goodsListHeader {
	width: 100%;
	margin: 0;
	padding: 0;
	display: -webkit-flex;
	display: flex;
	-webkit-flex: 1;
	flex: 1;
	flex-direction: row;
	background-image: url(../img/singleTimeBg.png);
	background-size: auto 100%;
}

.goodsListHeader li {
	list-style: none;
	color: #fff;
	font-size: 2.8vh;
	line-height: 6.5vh;
	margin-left: 3.5%;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 1;
	-webkit-align-items: flex-start;
	align-items: flex-start;
	display: -moz-box;
	-moz-box-pack: start;
	-moz-box-align: start;
	display: -webkit-box;
	-webkit-box-pack: start;
	-webkit-box-align: start;
	display: box;
	box-pack: start;
	box-align: start;
}

.goodsListContent {
	display: -webkit-flex;
	display: flex;
	-webkit-flex: 10;
	flex: 10;
	flex-direction: column;
	width: 100%;
	margin: 0 auto;
}

.goodsListContent a {
	-webkit-flex: 1;
	display: -webkit-flex;
	display: flex;
	color: #CBD199;
	margin: 0;
	padding: 0;
}
.goodsListContent .data-success{
	color: #11F675;
}

.goodsListContent .data-delay{
	color: #E5B732;
}

.goodsListContent .data-error{
	color: #FF3334;
}

.goodsListContent .contentBox {
	display: box;
    display: -webkit-box;
    display: -moz-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
	-webkit-box-flex: 1;
    -moz-box-flex: 1;
    -webkit-flex: 1;
    -ms-flex: 1;
    flex: 1;
    height:10%;
    margin: 0;
	padding: 0;
	overflow:hidden;
}

.goodsListContent ul:nth-child(even) {
	background-color: #01134d;
}

.goodsListContent .contentBox li {
	font-size: 2.8vh;
    margin-left: 3.5%;
    margin-top:1.4vh;
 	list-style: none;
	white-space:nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}

.contentBox-lione {
	-webkit-flex: 1;
}

.contentBox-litwo {
	-webkit-flex: 1;
}

.contentBox-lithreen {
	-webkit-flex: 1;
}